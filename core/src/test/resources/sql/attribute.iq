!set outputformat mysql
!use scott

# Define an attribute.
# Value and alias are same as for 'sal'.
select ename, define_attribute(sal, 'odd', mod(sal, 2) = 1)
from emp
where deptno = 10;
+--------+---------+
| ENAME  | SAL     |
+--------+---------+
| CLARK  | 2450.00 |
| KING   | 5000.00 |
| MILLER | 1300.00 |
+--------+---------+
(3 rows)

!ok

# Multiple attributes.
select
    define_attribute(ename, 'odd', mod(sal, 2) = 1),
    define_attribute(
      define_attribute(sal, 'odd', mod(sal, 2) = 1),
        'even', mod(sal, 2) = 1)
from emp
where deptno = 10;
+--------+---------+
| ENAME  | SAL     |
+--------+---------+
| CLARK  | 2450.00 |
| KING   | 5000.00 |
| MILLER | 1300.00 |
+--------+---------+
(3 rows)

!ok

# End attribute.iq
